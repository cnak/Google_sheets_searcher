{
  "name": "basic-search-stacksearch",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "11.5.0",
    "yarn": "1.12.3"
  },
  "dependencies": {
    "@shinnn/eslint-config-node": "^6.0.2",
    "axios": "^0.18.0",
    "eslint-config-chartjs": "^0.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.15.3",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-scripts": "2.1.3",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "chart.js": "^2.5.0",
    "event-stream": "^3.3.4",
    "express": "^4.16.4",
    "google-spreadsheet": "^2.0.7",
    "googleapis": "27",
    "lebab": "^3.0.3",
    "node-fetch": "^2.1.2",
    "nodemon": "^1.17.5"
  },
  "scripts": {
    "start": "npm-run-all -p watch-css start-node start-js",
    "eject": "react-scripts eject",
    "build-css": "node-sass-chokidar --include-path ./src/client/ --include-path ./node_modules src/client/ -o src/client",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src/client/ --include-path ./node_modules src/client -o src/client --watch --recursive",
    "start-js": "react-scripts start",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "precommit": "lint-staged",
    "start-node": "node src/server/index.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "lint-staged": {
    "src/client/*.{js,jsx,json,scss,css}": [
      "prettier --config .prettierrc --write",
      "git add"
    ]
  },
  "devDependencies": {
    "eslint": "^4.19.1 || ^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.0",
    "@babel/cli": "^7.0.0-rc.1",
    "@babel/core": "^7.0.0-rc.1",
    "@babel/node": "^7.0.0-rc.1",
    "@babel/preset-env": "^7.0.0-rc.1",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.3"
  }
}
